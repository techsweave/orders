{
    "name": "orders-service",
    "version": "1.0.0",
    "description": "Orders service for TechSWEave's emporioLambda platform",
    "main": "serverless.ts",
    "scripts": {
        "test": "mocha -r dotenv/config -r ts-node/register 'tests/**/*.ts'",
        "start": "./node_modules/.bin/serverless offline -s dev",
        "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline -s dev",
        "debugWindows": "SET export SLS_DEBUG=* && node --nolazy --inspect node_modules/serverless/bin/serverless offline -s dev"
    },
    "engines": {
        "node": ">=14.15.0"
    },
    "dependencies": {
        "@aws/dynamodb-data-mapper": "^0.7.3",
        "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
        "@middy/core": "^2.2.0",
        "@middy/http-json-body-parser": "^2.2.0",
        "@serverless/components": "^3.9.2",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "aws-sdk": "^2.918.0",
        "chai-json-schema": "^1.5.1",
        "http-status-codes": "^2.1.4",
        "json-schema-to-ts": "^1.6.4",
        "serverless": "^2.42.0",
        "serverless-dynamodb-local": "^0.2.39",
        "serverless-export-env": "^1.4.4",
        "serverless-offline": "^7.0.0",
        "serverless-offline-sns": "^0.7.0",
        "serverless-offline-sqs": "^4.1.1",
        "serverless-webpack": "^5.5.0",
        "source-map-support": "^0.5.19",
        "ts-loader": "^9.2.2",
        "ts-node": "^10.0.0",
        "tsconfig-paths-webpack-plugin": "^3.5.1",
        "utilities-techsweave": "^1.1.13",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@serverless/typescript": "^2.43.0",
        "@types/aws-lambda": "^8.10.76",
        "@types/chai": "^4.2.18",
        "@types/mocha": "^8.2.2",
        "@types/node": "^15.6.0",
        "chai": "^4.3.4",
        "eslint": "^7.27.0",
        "mocha": "^6.2.3",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.4",
        "webpack": "^5.37.1",
        "webpack-node-externals": "^3.0.0"
